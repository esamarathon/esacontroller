{
	"port": 3333,
	"youtube": {
		"enable": true,
		"delay": 600000,
		"buffers": {
			"beginning": 12,
			"end": 300
		},
		"templates": {
			"title": "#ESA17 Speedruns - {game} [{category}] by {playersString}",
			"description": "Generic Description for {game} {category}.\n\n{twitches}",
			"keywords": "ESA,ESA2017,EuropeanSpeedrunnerAssembly,{game},{playersString}"
		},
		"command": "/usr/bin/nice",
		"parameters": "-n 10 /usr/local/bin/esaupload --twitch -c \"/home/tech/esacontroller/youtube_publish_cache.txt\" -g \"{game}\" -t \"{title}\" -d \"{description}\" -k \"{keywords}\" -m \"/mnt/data/recordings_moved/{title}\" -s {start} -e {end} -o \"/mnt/data/recordings_moved/output_{title}.mp4\" /mnt/data/recordings_sorted"
	},
	"esavods": {
		"enable": false,
		"server": "api.esavods.com"
	},
	"speedcontrol": {
		"key": "<CHANGE THIS>",
		"host": "10.0.0.99",
		"port": 9090
	},
	"esarack": {
		"racks": [
			"10.0.0.11",
			"10.0.0.12",
			"10.0.0.13",
			"10.0.0.14"
		],
		"port": 8080
	}
}
