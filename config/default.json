{
	"port": 3333,
	"sharedkey": "ENTER_YOUR_SHARED_KEY_HERE",
	"youtube": {
		"enable": true,
		"buffers": {
			"beginning": 5,
			"end": 120
		},
		"templates": {
			"title": "ESA2016: {game} ({category}) by {playersString}",
			"description": "Generic Description for {game} {category}.",
			"keywords": "ESA,ESA2016,{game},{playersString}"
		},
		"command": "echo",
		"parameters": "--title \"{title}\" --desc \"{description}\" --keywords \"{keywords}\" -s {start} -e {end}"
	},
	"esavods": {
		"enable": false,
		"server": "api.esavods.com"
	},
	"speedcontrol": {
		"host": "localhost",
		"port": 9090,
		"key": "ENTER_YOUR_SHARED_KEY_HERE"
	}
}
