<div class="card" id="in1606">
	<header>
		<h3>IN1606</h3>
	</header>
	<form method="POST" action="/rack/in1606">
		<input type="hidden" name="rack" value="1">
		<fieldset>
			<label>Input</label>
			<select name="input">
				<option>No change</option>
				<% for (var i = 1; i < 7; i++) { %>
					<option <%= i == current.in1696.input ? "selected" : "" %>><%= i %></option>
				<% } %>
			</select>
		</fieldset>

		<fieldset class="fieldset">
			<legend>Cropping</legend>
			<label>Width</label>
			<input type="number" 
				name="width" min=10 max=4096
				value=<%= current.in1696.width %>> 

			<label>Height</label>
			<input type="number" 
				name="height" min=10 max=2400
				value=<%= current.in1696.height %>>

			<label>Horizontal Shift</label>
			<input type="number" 
				name="horizontalShift" min=-2048 max=2048
				value=<%= current.in1696.hshift %>>

			<label>Vertical Shift</label>
			<input type="number" 
				name="verticalShift" min=-1200 max=1200
				value=<%= current.in1696.vshift %>>
		</fieldset>

		<!--
		public int contrast;
		public int brightness;
		-->

		<input type="submit" value="Send"/>
	</form>
</div>
